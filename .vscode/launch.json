{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Function",
      "type": "coreclr",
      "request": "attach",
      "processName": "func",
      "preLaunchTask": "func host start",
      "justMyCode": true,
      "sourceLinkOptions": {
        "enabled": true
      },
      "sourceFileMap": {
        "/home/site/wwwroot": "${workspaceFolder}"
      }
    }
  ]
}